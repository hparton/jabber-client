{
  "name": "jabber",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --inline --hot --progress",
    "electron": "./node_modules/electron-prebuilt/dist/Electron.app/Contents/MacOS/Electron .",
    "package": "npm run build; npm run package-osx; npm run package-win; npm run package-linux;",
    "package-osx": "electron-packager . $npm_package_productName -platform=darwin --ignore='^/package$' --arch=x64 --app-version=$npm_package_version --version=0.36.7 --icon=assets/app.icns --out=package --asar=true --overwrite=force",
    "package-win": "electron-packager . $npm_package_productName --platform=win32 --ignore='^/package$' --arch=ia32 --app-version=$npm_package_version --version=0.36.7 --icon=assets/app.ico --out=package --asar=true --overwrite=force",
  },
  "build": {
     "publish": [{
         "provider": "github",
         "owner": "hparton",
         "repo": "jabber-client"
     }],
  },
  "productName": "Jabber",
  "version": "0.0.1",
  "author": "HParton",
  "description": "Jabber",
  "main": "electron.js",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.4",
    "electron-prebuilt": "^1.4.13",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "node-sass": "^4.5.3",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.16.0",
    "vue-loader": "^11.3.3",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^2.3.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.2",
    "electron-packager": "^8.7.1"
  },
  "dependencies": {
    "basscss": "^8.0.2",
    "basscss-forms": "^1.0.0",
    "feathers-authentication-client": "^0.3.2",
    "feathers-client": "^2.2.0",
    "feathers-hooks": "^2.0.1",
    "feathers-reactive": "^0.4.1",
    "feathers-socketio": "^2.0.0",
    "feathers-vuex": "^0.4.2",
    "rxjs": "^5.4.1",
    "socket.io-client": "^2.0.3",
    "vue": "^2.3.4",
    "vue-router": "^2.6.0",
    "vuex": "^2.3.1"
  }
}
